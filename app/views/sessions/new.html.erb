<%= render 'shared/title', title: 'Please Log In' %>

<div class="well">
<%= form_tag('/login') do%>
  <%= label_tag 'username' %>
  <%= text_field_tag 'username' %>
  <%= label_tag 'password' %>
  <%= password_field_tag 'password' %>
  <br/>
  <%= submit_tag 'Submit', class: 'btn btn-primary' %>
<% end %>
<div/>